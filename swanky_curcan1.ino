  digitalWrite(1, LOW);
    delay(800);
  digitalWrite(2,HIGH);
  delay(800); // Wait for 1000 millisecond(s)
  digitalWrite(2, LOW);
    delay(800);
  digitalWrite(3,HIGH);
  delay(800); // Wait for 1000 millisecond(s)
  digitalWrite(3, LOW);
    delay(800);
  digitalWrite(4,HIGH);
