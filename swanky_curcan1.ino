void setup()
{
  pinMode(0, OUTPUT);
  pinMode(1, OUTPUT);
  pinMode(2, OUTPUT);
  pinMode(3, OUTPUT);
  pinMode(4, OUTPUT);
 
}

void loop()
{
  digitalWrite(0,HIGH);
  delay(800); // Wait for 1000 millisecond(s)
  digitalWrite(0, LOW);
  delay(800);
 digitalWrite(1,HIGH);
